{
    "name": "dick/settings",
    "description": "Application settings interface for Laravel 5 using Dick CRUD.",
    "keywords": [
        "tabacitu",
        "dick settings",
        "updivision"
        "dick",
        "manage settings",
        "settings admin"
    ],
    "homepage": "https://github.com/tabacitu/settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Tabacitu",
            "email": "hello@tabacitu.ro",
            "homepage": "http://www.tabacitu.ro",
            "role": "Dick Master"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0",
        "dick/crud": "0.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Dick\\Settings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dick\\Settings\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
